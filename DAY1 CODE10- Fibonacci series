package Day1;
import java.util.Scanner;

public class Fibonacci_series {

	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a number");
		int num = scan.nextInt();
		scan.close();
		int a = 0;
		int b=1;
		int i=0;
		
		while(i<num)
		{			
			System.out.print(a + " ");
			int c = a+b;
			a=b;
			b=c;
			i++;
		}		
	}
}
